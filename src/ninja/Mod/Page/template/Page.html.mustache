<!DOCTYPE html>
<html>
<head>

	<meta http-equiv="content-type" content="text/html; charset=UTF-8" />
	<base href="{{Model.baseHref}}" />

	{{#meta}}
	<meta name="{{name}}" content="{{content}}" />
	{{/meta}}

	<title>{{M.title}}</title>

	{{#View.getWithRoot_css}}
	{{#onlyIf}}<!-- IF {{onlyIf}} -->{{/onlyIf}}
	<link rel="stylesheet" {{#media}}media={{media}}{{/media}} href="{{href}}" />
	{{#onlyIf}}<!-- IF {{onlyIf}} -->{{/onlyIf}}
	{{/View.getWithRoot_css}}

	{{#View.getScriptsHead}}
	{{#src}}<script src="{{{src}}}" ></script>{{/src}}
	{{/View.getScriptsHead}}
	<script>
	{{#View.getScriptsHead}}
	{{#code}}{{{code}}}{{/code}}
	{{/View.getScriptsHead}}
	</script>

</head>
<body {{#cssId}}id="#{{cssId}}"{{/cssId}} class="n-mod-page{{#View.getWithRoot_cssClasses}} {{.}}{{/View.getWithRoot_cssClasses}}" {{#View.getWithRoot_cssStyle}}style="{{{.}}}"{{/View.getWithRoot_cssStyle}}>

{{{ Contents.header }}}
{{{ Contents.columns }}}
{{{ Contents.footer }}}

{{#View.getScriptsFoot}}
{{#src}}<script src="{{{src}}}" ></script>{{/src}}
{{/View.getScriptsFoot}}
<script>
{{#View.getScriptsFoot}}
{{#code}}{{{code}}}{{/code}}
{{/View.getScriptsFoot}}
</script>

</body>
</html>
